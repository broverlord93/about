{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@src/*": [
        "src/*"
      ]
    },
    "include": [
      "src"
    ]
  }
}
